{
  "version": 2,
  "builds": [
    {
      "src": "src/index.js",  // Path to your backend entry file
      "use": "@vercel/node"   // Tells Vercel you're using Node.js
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "src/index.js"  // Path to the entry point for all routes
    }
  ]
}
